{
  "manifest_version": 2,

  "name": "Snippets",
  "description": "This extension allows the user to search Snippets.",
  "version": "1.0",

  "browser_action": {
    "default_title": "Snippets",
    "short_name": "Snippets",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://laravel-snippets.herokuapp.com/"
  ]
}
